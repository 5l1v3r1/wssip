{
  "name": "wssip",
  "version": "1.0.7",
  "description": "Intercepting Proxy Application to capture, modify and send custom data from one WebSocket host to another, primarily for penetration testing. Supports WebSocket RFC6455 implementations and Socket.io.",
  "main": "lib/electron.js",
  "bin": {
    "wssip": "lib/main.js"
  },
  "scripts": {
    "start": "electron .",
    "prepublishOnly": "npm run compile",
    "web": "node lib/main.js web",
    "debug": "DEBUG=wssip:* electron .",
    "debug:win": "set DEBUG=wssip:* && electron . --devtools",
    "debug:full": "npm run compile:dev && DEBUG=wssip:* electron . --devtools",
    "compile": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "compile:dev": "NODE_ENV=development webpack --config webpack.config.js",
    "compile:dev-watch": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "npm run compile && npm run build:mac && npm run build:linux && npm run build:win",
    "build:mac": "npm run build:mac-x64",
    "build:mac-x64": "build -m --x64",
    "build:linux": "npm run build:linux-x64 && npm run build:linux-ia32 && npm run build:linux-armv71",
    "build:linux-x64": "build -l --x64",
    "build:linux-ia32": "build -l --ia32",
    "build:linux-armv71": "build -l --armv71",
    "build:win": "npm run build:win-x64 && npm run build:win-ia32 && npm run build:win-armv71",
    "build:win-x64": "build -w --x64",
    "build:win-ia32": "build -w --ia32",
    "build:win-armv71": "build -w --armv71",
    "lint": "npm run lint:src && npm run lint:lib",
    "lint:src": "eslint src",
    "lint:lib": "eslint lib",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nccgroup/wssip.git"
  },
  "bugs": {
    "url": "https://github.com/nccgroup/wssip/issues"
  },
  "keywords": [
    "ws",
    "wssip",
    "websocket",
    "websockets",
    "socketio",
    "socket.io",
    "interceptor",
    "proxy",
    "mitm",
    "http",
    "https"
  ],
  "author": "Samantha Chalker <samantha@chalker.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "conf": "^1.1.0",
    "electron": "1.7.x",
    "electron-window-state": "^4.1.1",
    "hexy": "^0.2.9",
    "lodash": "^4.17.4",
    "node-forge": "^0.7.1",
    "tunnel-agent": "^0.6.0",
    "ws": "^2.3.1",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "electron-builder": "^17.5.0",
    "electron-react-devtools": "^0.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "material-ui": "^0.18.0",
    "mocha": "^3.3.0",
    "react": "^15.5.4",
    "react-autobind": "^1.0.6",
    "react-data-grid": "^2.0.41",
    "react-dom": "^15.5.4",
    "react-scripts": "^0.9.5",
    "react-tap-event-plugin": "^2.0.1",
    "webpack": "^2.5.1"
  },
  "optionalDependencies": {
    "debug": "^2.6.6",
    "node-libcurl": "^1.1.0"
  },
  "engines": {
    "node": ">=7.9.0"
  },
  "build": {
    "appId": "com.nccgroup.wssip",
    "directories": {
      "buildResources": "./build/staging",
      "output": "./dist"
    },
    "dmg": {},
    "files": [
      "app/app.html",
      "app/app.js",
      "lib/*.js",
      "node_modules/**/*",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "LICENSE",
      "package.json"
    ],
    "linux": {
      "category": "Utility",
      "icon": "./build/icons/512x512.png",
      "target": [
        "deb",
        "rpm",
        "zip"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "./build/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "productName": "WSSiP",
    "win": {
      "icon": "./build/icon.ico",
      "target": [
        "portable",
        "zip"
      ]
    }
  }
}
